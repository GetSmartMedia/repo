<requires>
<import addon="xbmc.python" version="2.1.0"/>
<import addon="script.module.addon.common" optional="true"/>
</requires>
<extension point="xbmc.python.script" library="default.py">
<provides>executable</provides>
</extension>
<extension point="xbmc.service" library="service.py" start="startup"></extension>
<extension point="xbmc.addon.metadata">
<summary>GetSmartMedia NOTIFICATIONS</summary>
<description>
Receive Important GetSmartMedia Notifications. Unsubscribe within Add-on Settings.
</description>
<disclaimer>
We take no responsibility for any problems this plugin might in any way, form, shape or size cause. Modified code from the TVADDONS.AG Notification add-on.
</disclaimer>
<language>en</language>
<platform>all</platform>
<forum>http://www.GetSmartMediaWorldwide.com/forum</forum>
<website>http://www.GetSmartMediaWorldwide.com</website>
<source/>
<email/>
</extension>
</addon>
